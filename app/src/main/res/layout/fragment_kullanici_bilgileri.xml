<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".KullaniciBilgileriFragment">


    <TextView
        android:id="@+id/adttxt7"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="29dp"
        android:layout_marginEnd="37dp"
        android:text="Email Adresi"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/emailtxt"
        app:layout_constraintEnd_toStartOf="@+id/emailtxt"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/adttxt5"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="29dp"
        android:layout_marginEnd="84dp"
        android:layout_marginBottom="23dp"
        android:text="Soyad: "
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/telefontxt"
        app:layout_constraintEnd_toStartOf="@+id/soyadtxt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adttxt3" />

    <TextView
        android:id="@+id/adttxt6"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="29dp"
        android:layout_marginEnd="13dp"
        android:text="Telefon Numarısı"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/telefontxt"
        app:layout_constraintEnd_toStartOf="@+id/telefontxt"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/emailtxt"
        android:layout_width="185dp"
        android:layout_height="0dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="43dp"
        android:text="Email Adresiniz"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/adttxt7"
        app:layout_constraintTop_toBottomOf="@+id/telefontxt" />

    <TextView
        android:id="@+id/telefontxt"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginEnd="69dp"
        android:layout_marginBottom="24dp"
        android:text="Telefon Numarınız"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/emailtxt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/adttxt6"
        app:layout_constraintTop_toBottomOf="@+id/adttxt5" />

    <TextView
        android:id="@+id/soyadtxt"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="39dp"
        android:text="Soyadınız"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/adttxt5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/adttxt5"
        app:layout_constraintTop_toTopOf="@+id/adttxt5" />

    <Button
        android:id="@+id/button4"
        android:layout_width="398dp"
        android:layout_height="0dp"
        android:layout_marginStart="6dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="2dp"
        android:onClick="KullaniciGuncelleAc"
        android:text="KULLANICI BİLGİLERİNİ GÜNCELLE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/adttxt"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="39dp"
        android:text="Adınız"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/adttxt3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/adttxt3"
        app:layout_constraintTop_toTopOf="@+id/adttxt3" />

    <TextView
        android:id="@+id/adttxt3"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="26dp"
        android:text="Ad: "
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/adttxt5"
        app:layout_constraintEnd_toStartOf="@+id/adttxt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>